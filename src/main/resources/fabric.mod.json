{
  "schemaVersion": 1,
  "id": "gourmetmod",
  "version": "${version}",

  "name": "Gourmet Mod",
  "description": "This mod ports all the food from regular Minecraft and adds foods of my preference while keeping a vanilla feeling.",
  "authors": [
    "Kashimo Hajime"
  ],
  "contact": {
    "homepage": "",
    "sources": ""
  },

	"icon": "icon.png",
  "license": "CC0-1.0",

  "environment": "*",
  "entrypoints": {
    "main": [
			"kashimo.gourmetmod.GourmetMod"
    ],
